Use uo;

include "include/spawnnet";



program global_hide(who)


local runeserial := GetGlobalProperty("runeserial");

local counter:=0,dummy;
foreach rune in runeserial
	if(!rune)
		continue;
	endif
	dummy := SystemFindObjectBySerial(rune);
	if(!dummy)
		continue;
	endif
	dummy.graphic := 0x1;
	counter:=counter+1;
endforeach

SendSysMessage(who,"Insg. "+counter+" Runen versteckt.");
endprogram